<?xml version="1.0" encoding="ASCII"?>
<atl2java:Transformation
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:atl2java="http://atl2java.ame13.big.tuwien.ac.at/1.0"
    xsi:schemaLocation="http://atl2java.ame13.big.tuwien.ac.at/1.0 ../../at.ac.tuwien.big.ame13.atl2java/model/atl2java.ecore"
    name="Book2EBook">
  <rules name="Model2Model">
    <inputPattern>
      <inputPatternElements
          name="ma"
          type="Model"
          metamodel="//@sourceModel"/>
    </inputPattern>
    <outputPattern>
      <outputPatternElements
          name="mb"
          type="Model"
          metamodel="//@targetModel">
        <bindings
            xsi:type="atl2java:ResolveBinding"
            feature="b"
            inputPatternElement="//@rules.0/@inputPattern/@inputPatternElements.0"
            value="a"/>
        <bindings
            xsi:type="atl2java:ResolveBinding"
            feature="y"
            inputPatternElement="//@rules.0/@inputPattern/@inputPatternElements.0"
            value="x"/>
      </outputPatternElements>
    </outputPattern>
  </rules>
  <rules name="A2B">
    <inputPattern>
      <inputPatternElements
          name="a"
          type="A"
          metamodel="//@sourceModel"/>
    </inputPattern>
    <outputPattern>
      <outputPatternElements
          name="b"
          type="B"
          metamodel="//@targetModel">
        <bindings
            xsi:type="atl2java:NavigationBinding"
            feature="id"
            inputPatternElement="//@rules.1/@inputPattern/@inputPatternElements.0"
            value="name"/>
      </outputPatternElements>
    </outputPattern>
  </rules>
  <rules name="Book2Ebook">
    <inputPattern>
      <inputPatternElements
          name="x"
          type="Book"
          metamodel="//@sourceModel"/>
    </inputPattern>
    <outputPattern>
      <outputPatternElements
          name="y"
          type="Ebook"
          metamodel="//@targetModel">
        <bindings
            xsi:type="atl2java:NavigationBinding"
            feature="label"
            inputPatternElement="//@rules.2/@inputPattern/@inputPatternElements.0"
            value="title"/>
        <bindings
            xsi:type="atl2java:NavigationBinding"
            feature="info"
            inputPatternElement="//@rules.2/@inputPattern/@inputPatternElements.0"
            value="author"/>
        <bindings
            xsi:type="atl2java:NavigationBinding"
            feature="date"
            inputPatternElement="//@rules.2/@inputPattern/@inputPatternElements.0"
            value="published"/>
        <bindings
            xsi:type="atl2java:PrimitiveBinding"
            feature="category"
            value="Thriller"/>
      </outputPatternElements>
    </outputPattern>
  </rules>
  <sourceModel
      path="testmodel"
      name="A"
      metamodel="Book.ecore"/>
  <targetModel
      path="testmodel"
      name="B"
      metamodel="Ebook.ecore"/>
</atl2java:Transformation>
