<?xml version="1.0" encoding="ASCII"?>
<atl2java:Transformation
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:atl2java="http://atl2java.ame13.big.tuwien.ac.at/1.0"
    xsi:schemaLocation="http://atl2java.ame13.big.tuwien.ac.at/1.0 ../../at.ac.tuwien.big.ame13.atl2java/model/atl2java.ecore"
    name="A2B">
  <rules name="Model2Model">
    <inputPattern>
      <inputPatternElements
          name="ma"
          type="Model"
          metamodel="//@sourceModel"/>
    </inputPattern>
    <outputPattern>
      <outputPatternElements
          name="mb"
          type="Model"
          metamodel="//@targetModel">
        <bindings
            xsi:type="atl2java:ResolveBinding"
            feature="b"
            inputPatternElement="//@rules.0/@inputPattern/@inputPatternElements.0"
            value="a"/>
      </outputPatternElements>
    </outputPattern>
  </rules>
  <rules name="A2B">
    <inputPattern>
      <inputPatternElements
          name="a"
          type="A"
          metamodel="//@sourceModel"/>
    </inputPattern>
    <outputPattern>
      <outputPatternElements
          name="b"
          type="B"
          metamodel="//@targetModel">
        <bindings
            xsi:type="atl2java:NavigationBinding"
            feature="id"
            inputPatternElement="//@rules.1/@inputPattern/@inputPatternElements.0"
            value="name"/>
      </outputPatternElements>
    </outputPattern>
  </rules>
  <sourceModel
      path="testmodel"
      name="A"
      metamodel="A.ecore"/>
  <targetModel
      path="testmodel"
      name="B"
      metamodel="B.ecore"/>
</atl2java:Transformation>
